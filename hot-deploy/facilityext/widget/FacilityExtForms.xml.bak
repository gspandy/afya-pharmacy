<?xml version="1.0" encoding="UTF-8"?>
<forms xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/widget-form.xsd">
	<form name="SearchTransformation" type="single" target="SearchTransformation"
		separate-columns="true">
		<auto-fields-entity entity-name="ProductTransfer"
			default-field-type="find" />
		<field name="productTransferId" title="SKU Transfer Id">
			<text />
		</field>
		<field name="quantityRequired">
			<text />
		</field>
		<field name="fromProductId" title="From SKU Id"></field>
		<field name="toProductId" title="To SKU Id"></field>

		<field name="status">
			<drop-down allow-empty="true">
				<option key="REQUESTED" />
				<option key="SCHEDULED" />
				<option key="COMPLETED" />
			</drop-down>
		</field>
		<field name="noConditionFind">
			<hidden value="Y" />
		</field>
		<field name="submitButton" title="${uiLabelMap.CommonSearch}">
			<submit button-type="button" />
		</field>
	</form>

	<form name="ListTransformation" type="list" separate-columns="true"
		list-name="listIt" paginate-target="iSearchTransformationsueSeach"
		paginate="true" odd-row-style="alternate-row" header-row-style="header-row-2"
		default-table-style="basic-table hover-bar">
		<actions>
			<service service-name="performFind">
				<field-map field-name="inputFields" from-field="parameters" />
				<field-map field-name="entityName" value="ProductTransfer" />
				<field-map field-name="noConditionFind" value="Y" />
			</service>
		</actions>
		<auto-fields-entity entity-name="ProductTransfer"
			default-field-type="display" />
		<field name="productTransferId" title="SKU Transfer Id">
			<hyperlink
				target="ViewTransformation?productTransferId=${productTransferId}"
				description="${productTransferId}" />
		</field>
		<field name="fromProductId" title="From SKU Id"></field>
		<field name="toProductId" title="To SKU Id"></field>
	</form>
	<form name="SearchProductBundle" type="single" target=""
		default-entity-name="ProductBundle">
		<auto-fields-entity entity-name="ProductBundle"
			default-field-type="find" />

		<field name="SearchBlank">
			<hidden value="yes" />
		</field>

		<field name="productBundleId" title="SKU Bundle Id"></field>
		<field name="search">
			<submit />
		</field>
	</form>

	<form name="ListProductBundle" target="" type="list"
		odd-row-style="alternate-row" list-name="list" default-table-style="basic-table hover-bar"
		separate-columns="true" override-list-size="${listSize}">
		<actions>
			<service service-name="performFindList">
				<field-map field-name="inputFields" from-field="parameters" />
				<field-map field-name="entityName" value="ProductBundle" />

				<field-map field-name="noConditionFind" value="Y" />
			</service>
		</actions>

		<auto-fields-entity entity-name="ProductBundle"
			default-field-type="display" />
		<field name="productBundleId">
			<hyperlink target="NewBundlingTxn1" description="${productBundleId}"
				also-hidden="true">
				<parameter param-name="productBundleId" value="${productBundleId}" />
			</hyperlink>

		</field>
		<field name="productBundleId" title="SKU Bundle Id"></field>

	</form>
	<form name="MergeForm" target="merge" type="single">

		<field name="fromInventoryItemId">
			<lookup target-form-name="LookupInventoryItems"></lookup>
		</field>
		<field name="toInventoryItemId">
			<lookup target-form-name="LookupInventoryItems"></lookup>
		</field>

		<field name="locationSeqIdTo">
			<lookup target-form-name="LookupFacilityLocation?storageId=CASE"></lookup>
		</field>
		<field name="merge">
			<submit />
		</field>
	</form>
	<form name="TransformationTxn" type="single" target="SaveTransferTxn"
		default-map-name="transferTxn" default-entity-name="ProductTransferTxn">
		<field name="productTransferId">
			<hidden value="${productTransferId}" />
		</field>
		<field name="fromInventoryItemId">
			<drop-down current="selected">
				<entity-options description="${inventoryItemId}"
					entity-name="ProductTransferLocation" key-field-name="inventoryItemId">
					<entity-constraint name="productTransferId"
						value="${productTransferId}" />
					<entity-constraint name="type" value="SRC" />
				</entity-options>
			</drop-down>
		</field>
		<field name="toLocationId">
			<drop-down current="selected">
				<entity-options description="${locationId}"
					entity-name="ProductTransferLocation" key-field-name="locationId">
					<entity-constraint name="productTransferId"
						value="${productTransferId}" />
					<entity-constraint name="type" value="DEST" />
				</entity-options>
			</drop-down>
		</field>
		<field name="quantityDone">
			<text />
		</field>


		<field name="palletizeSource">
			<hidden value="SPLITTING" />
		</field>
		<field name="submit" title="Add Transformation Txn"
			use-when="${productTransfer.status != 'COMPLETED'}" event="onclick"
			action="javascript:return validateQtyDone();">
			<submit />
		</field>
	</form>

	<form name="ListTransformationTxn" type="list" list-name="transferTxns"
		separate-columns="true" odd-row-style="alternate-row"
		header-row-style="header-row-2" default-table-style="basic-table hover-bar">
		<actions>
			<service service-name="performFind" result-map="result"
				result-map-list="listIt">
				<field-map field-name="inputFields" from-field="parameters" />
				<field-map field-name="entityName" value="ProductTransferTxn" />
				<field-map field-name="noConditionFind" value="Y" />
			</service>
		</actions>

		<field name="txnId">
			<display also-hidden="true" />
		</field>
		<field name="quantityDone">
			<display />
		</field>
		<field name="productTransferId">
			<hidden value="${parameters.productTransferId}" />
		</field>
		<field name="fromInventoryItemId" title="From Inventory">
			<display />
		</field>
		<field name="toLocationId" title="To Location">
			<display />
		</field>
		<field name="createdStamp" title="Date">
			<display />
		</field>
		<field name="status">
			<hyperlink
				target="TransformationTxnStatusChange?txnId=${txnId}&amp;fromInventoryItemId=${fromInventoryItemId}&amp;productTransferId=${productTransferId}&amp;toLocationId=${toLocationId}&amp;quantityDone=${quantityDone}"
				description="Close Txn"></hyperlink>
		</field>
	</form>

	<form name="ProductBundleForm" type="single" target="ProductBundle">
