			<display></display>
		</field>

		<field name="toLocationId" title="Putaway Location">
			<lookup target-form-name="LookUpLocation"></lookup>
		</field>
		<field name="tagCode">
			<display also-hidden="true"></display>
		</field>
		<field name="quantityToBundle" title="Quantity Now"
			tooltip="Quantity that is required in this transaction.">
			<text default-value="0"></text>
		</field>
		<field name="palletize">
			<check></check>
		</field>
		<field name="Continue" event="onclick"
			action="javascript:
			   var qtyBundle= parseInt(document.NewBundlingTxn1.quantityToBundle.value);
			   var qtyBundleRaw = document.NewBundlingTxn1.pendingQuantity.value;
			   var bundle = true;
			   var qtyPending = 0;

			   if (qtyBundleRaw.indexOf(',') != -1)
			   {
					qtyBundleRaw = qtyBundleRaw.replace(/,/g,'');
					qtyPending= parseInt(qtyBundleRaw);
			   }
			   else 
			   {
			   	qtyPending = parseInt(qtyBundleRaw);
			   }

			   if (isNaN(qtyBundle))
			    {
				alert('Invalid Value Specified. Only numeric value is allowed.');
				bundle = false;
				document.NewBundlingTxn1.quantityToBundle.focus();
			    }

			    if( qtyBundle &gt; qtyPending)
				{
					alert('The Quantity to be Bundled should be less than Quantity to make or there is no pending quantity left to bundle.');
					bundle= false;
					document.NewBundlingTxn1.quantityToBundle.focus();
				}
				return bundle;">

			<submit />
		</field>
	</form>
